<Application x:Class="CommunalPayments.Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CommunalPayments.Wpf"
             xmlns:viewModels="clr-namespace:CommunalPayments.Wpf.ViewModels"
             StartupUri="/Views/Main/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/GroupBox/GroupBoxDefaultStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/TextBox/TextBoxDefaultStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <viewModels:ViewModelLocator x:Key="Locator" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
